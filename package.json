{
  "name": "@fjandin/config-man",
  "version": "1.1.0",
  "description": "Config manager",
  "main": "index.js",
  "author": "Ren√© Bischoff <rene.bischoff@gmail.com>",
  "license": "MIT",
  "private": false,
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/"
    ],
    "report-dir": "./coverage",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "scripts": {
    "peer": "yarn add --peer aws-sdk",
    "tslint": "tslint -c tslint.json './src/**/*.ts' --project tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "CM_test1_test1a=env CM_env_unknown=test nyc mocha --opts ./test/mocha.opts -r ts-node/register",
    "test:watch": "nyc mocha --opts ./test/mocha-w.opts -r ts-node/register",
    "build": "rm -rf dist; tsc; cp package.json ./dist; echo '//registry.npmjs.org/:_authToken=${NPM_TOKEN}' > ./dist/.npmrc"
  },
  "dependencies": {
    "flat": "4.1.0"
  },
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/jest": "24.0.19",
    "@types/node": "^10.14.21",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.6.4"
  },
  "peerDependencies": {
    "aws-sdk": "^2.550.0"
  }
}
